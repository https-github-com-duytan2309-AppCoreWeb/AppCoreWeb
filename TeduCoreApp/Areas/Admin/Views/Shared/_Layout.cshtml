    <script src="~/admin/js/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
    <!-- iCheck -->
    <script src="~/admin/js/vendors/iCheck/icheck.min.js"></script>
    <!-- Skycons -->
    <script src="~/admin/js/vendors/skycons/skycons.js"></script>
    <!-- Flot -->
    <script src="~/admin/js/vendors/Flot/jquery.flot.js"></script>
    <script src="~/admin/js/vendors/Flot/jquery.flot.pie.js"></script>
    <script src="~/admin/js/vendors/Flot/jquery.flot.time.js"></script>
