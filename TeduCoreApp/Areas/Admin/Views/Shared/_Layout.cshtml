    <script src="~/admin/js/vendors/notifyjs/dist/styles/metro/notify-metro.js"></script>

    <!-- Custom Theme Scripts -->
    <script src="~/admin/build/js/custom.min.js"></script>

    <!-- Custom Theme Scripts -->
    <script src="~/admin/js/vendors/ckeditor/ckeditor.js"></script>
    <!--Boobox-->
    <script src="~/admin/js/vendors/bootbox/bootbox.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>