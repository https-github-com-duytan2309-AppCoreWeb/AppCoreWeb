    <script src="~/admin/js/vendors/jsrender/jsrender.min.js"></script>

    <script src="~/admin/js/vendors/syncfusion-javascript/Scripts/ej/web/ej.web.all.min.js"></script>
    @*<script src="~/client/js/ejs/properties.js"></script>
        <ej-script-manager></ej-script-manager>*@

    <!-- Datatables -->
    <script src="~/admin/js/vendors/datatables.net/js/jquery.dataTables.js"></script>
