    <script src="~/admin/js/vendors/flot-spline/js/jquery.flot.spline.min.js"></script>
    <script src="~/admin/js/vendors/flot.curvedlines/curvedLines.js"></script>
    <!-- DateJS -->
    <script src="~/admin/js/vendors/DateJS/build/date.js"></script>
