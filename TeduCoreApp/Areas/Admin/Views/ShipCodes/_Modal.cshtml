<div id="modal-detail" class="modal fade in" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog  modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title">Tạo Mới/Sửa</h4>
            </div>
            <div class="modal-body">
                <div id="horizontal-form">
                    <form class="form-horizontal" role="form" id="frmMaintainance">
                        <div class="form-group">
                            <input type="hidden" id="hidId" value="0" />
                            <label for="txtCarriers" class="col-sm-2 control-label ">Nhà vận chuyển</label>
                            <div class="col-sm-10">
                                <input type="text" name="Carriers" class="form-control" id="txtCarriers">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="dateDeliveryTime" class="col-sm-2 control-label ">Thời gian</label>
                            <div class="col-md-3 col-sm-3 col-xs-12 form-group">
                                <input type="text" id="dateDeliveryTime" class="form-control" placeholder="Thời Gian Vận Chuyển ...">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="txtCollectionFee" class="col-sm-2 control-label ">Chi phí vận chuyển</label>
                            <div class="col-sm-10">
                                <input type="text" name="txtCollectionFee" class="form-control" id="txtCollectionFee" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="txtZipCode" class="col-sm-2 control-label ">Mã ZipCode</label>
                            <div class="col-sm-10">
                                <input type="text" name="txtZipCode" class="form-control" id="txtZipCode" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label ">Địa Chỉ</label>
                            <div class="col-sm-10">
                                <select id="selectIdAddress" class="form-control">
                                    <option>
                                        1
                                    </option>
                                    <option>
                                        2
                                    </option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="txtTotal" class="col-sm-2 control-label">Tổng Chi Phí</label>
                            <div class="col-sm-10">
                                <input id="txtTotal" class="form-control" />
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <div class="modal-footer">
                <div class="col-sm-offset-2 col-sm-10">
                    <button type="button" id="btnSave" class="btn btn-success">Save</button>
                    <button type="button" data-dismiss="modal" class="btn btn-danger">Close</button>
                </div>
            </div>
        </div>
    </div>
</div>